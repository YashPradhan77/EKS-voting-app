{
   "Version": "2012-10-17",
   "Statement": [
       {
           "Effect": "Allow",
           "Action": "sts:AssumeRoleWithWebIdentity",
           "Principal": {
               "Federated": "arn:aws:iam::898896902478:oidc-provider/token.actions.githubusercontent.com"
           },
           "Condition": {
               "StringEquals": {
                   "token.actions.githubusercontent.com:aud": [
                       "sts.amazonaws.com"
                   ]
               },
               "StringLike": {
                   "token.actions.githubusercontent.com:sub": [
                       "repo:YashPradhan77/EKS-voting-app:*"
                   ]
               }
           }
       }
   ]
}